{
  "name": "authentication-api",
  "version": "0.0.31",
  "description": "Authentication API",
  "main": "index.js",
  "scripts": {
    "buildSample": "babel ./sample/ --out-dir dist/sample --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "buildSrc": "babel ./src --out-dir dist/src --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "clean": "rimraf dist/",
    "sample": "npm run clean && npm run buildSample && npm run buildSrc && node dist/sample/server.js",
    "prepublish": "npm run clean && npm run buildSrc"
  },
  "author": "Antonio Pol",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.1.2",
    "morgan": "^1.9.0"
  },
  "dependencies": {
    "rimraf": "^2.6.2",
    "sample": "0.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antoniopol06/authentication-api"
  }
}
